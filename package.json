{
  "name": "cloudflare-remix-accelerate-local-debug",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "npm-run-all -p dev:*",
    "build": "remix vite:build",
    "deploy": "npm run build && wrangler pages deploy",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev ./build/client",
    "typecheck": "tsc",
    "typegen": "wrangler types",
    "preview": "npm run build && wrangler pages dev",
    "cf-typegen": "wrangler types",
    "prisma:migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate --no-engine",
    "dev:docker": "docker compose -f docker/docker-compose.yml up -d",
    "dev:start": "remix vite:dev",
    "dev:proxy": "prisma-accelerate-local postgresql://postgres:password@localhost:5432/postgres?schema=accelerate-test -p 8000 -t"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^2.12.0",
    "@remix-run/cloudflare-pages": "^2.12.0",
    "@remix-run/react": "^2.12.0",
    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241011.0",
    "@prisma/client": "^5.20.0",
    "@remix-run/dev": "^2.12.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "prisma": "^5.20.0",
    "prisma-accelerate-local": "^1.1.19",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1",
    "wrangler": "3.57.1"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/SoraKumo001/cloudflare-remix-accelerate",
  "author": "SoraKumo <info@croud.jp>",
  "license": "MIT"
}
